ec-canvas {
  width: 100%;
  height: 100%;
  padding:300rpx,0rpx,10rpx,10rpx;
}

#mychart-dom-map{
  width:100%;
  height:800rpx;
  padding:100rpx,0rpx,20rpx,20rpx;
}

.button-sp-area{
  margin: 0 auto;
  width: 50%;
  padding-top:10rpx;
}

.word2{
  font-size:18px;      /*设置文字字号*/
  color:red;           /*设置文字颜色*/
  font-weight:bold;    /*设置字体加粗*/
  font-family:"微软雅黑";  
}

.word1{
  font-size:19px;      /*设置文字字号*/
  color:black;           /*设置文字颜色*/
  font-weight:bold;    /*设置字体加粗*/
  font-family:"微软雅黑";  
}

.container {
  position: absolute;
  top: 10rpx;
  bottom: 0;
  left: 0;
  right: 0;

  height: 90%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
 
  box-sizing: border-box;
} 
